<Window x:Class="StoreUI.CreateUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StoreUI"
        mc:Ignorable="d"
        Title="CreateUser" Height="290" Width="300">
    <StackPanel>
        <Label Margin="15 0 15 15">Username:</Label>
        <TextBox Name="UsernameTextBox" Margin="15 0 15 15" Padding="5">admin</TextBox>
        <Label Margin="15 0 15 15">Password:</Label>
        <PasswordBox Name="PasswordBox" Margin="15 0 15 15" Padding="5" Password="admin"></PasswordBox>
        <ComboBox Name="UserComboBox" SelectedIndex="1"  Margin="15 0 15 15" Padding="5">
            <ComboBoxItem>Customer</ComboBoxItem>
            <ComboBoxItem>Admin</ComboBoxItem>
            <ComboBoxItem>Moderator</ComboBoxItem>
        </ComboBox>
        <Button Name="CreateButton" Margin="15 0 15 15" Padding="5" Click="CreateButton_Click">Create</Button>
    </StackPanel>
</Window>
